<meta_metadata_repository name="lolz"
  package="ecologylab.bigsemantics.generated.library.lolz" default_user_agent_name="chrome_2">

  <meta_metadata name="lolz" extends="creative_work" parser="xpath" user_agent_name="google_bot_0"
    comment="The LOLZ class">
    <scalar name="title">
      <xpath>//div[@id='mainbody']/div[@id='pane2']/div/h1/a</xpath>
    </scalar>
    <scalar name="caption" scalar_type="String">
      <xpath>//div[@id='md']/div/p[2]</xpath>
    </scalar>
    <scalar name="picture" scalar_type="ParsedURL">
      <xpath>//div[@id='md']/div/p[1]/img/@src</xpath>
    </scalar>
    <scalar name="might_like" scalar_type="ParsedURL">
      <xpath>//div[@id='md']/div/p[3]/a/@href</xpath>
    </scalar>
  </meta_metadata>


  <meta_metadata name="cats" type="lolz">
    <selector url_path_tree="http://icanhascheezburger.com/" />
    <selector url_path_tree="http://icanhas.cheezburger.com/" />
    <semantic_actions>
      <get_field name="caption" />
      <get_field name="picture" />
      <create_and_visualize_img_surrogate>
        <arg value="caption" name="caption" />
        <arg value="picture" name="image_purl" />
      </create_and_visualize_img_surrogate>
      <get_field name="might_like" />
      <parse_document>
        <arg value="might_like" name="location" />
      </parse_document>
    </semantic_actions>
  </meta_metadata>


